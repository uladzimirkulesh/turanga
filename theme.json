				{
					"color": "rgba(255, 255, 255, 0.03)",
					"name": "Contrast Secondary Background",
					"slug": "contrast-secondary-bg"
				},
				{
					"color": "rgb(30, 30, 30)",
					"name": "Grey",
					"slug": "grey"
				},
				{
					"color": "#ff0000",
					"name": "Danger",
					"slug": "danger"
				}
			]
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "100%"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "calc(var(--wp--style--block-gap) / 2)",
					"slug": "10",
					"name": "1"
				},
				{
					"size": "var(--wp--style--block-gap)",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "clamp(2.25rem, 0.94vw + 2.06rem, 3rem)",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "clamp(3rem, 1.88vw + 2.63rem, 4.5rem)",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "clamp(4.5rem, 1.88vw + 4.13rem, 6rem)",
