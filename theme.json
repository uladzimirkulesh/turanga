					"slug": "base"
				},
				{
					"color": "rgb(255, 255, 255)",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "rgba(255, 255, 255, 0.65)",
					"name": "Contrast Text",
					"slug": "contrast-text"
				},
				{
					"color": "rgba(255, 255, 255, 0.45)",
					"name": "Contrast Secondary Text",
					"slug": "contrast-secondary-text"
				},
				{
