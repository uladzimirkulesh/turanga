						"min": "1.125rem",
						"max": "1.3125rem"
					},
					"name": "Large",
					"size": "1.3125rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.3125rem",
						"max": "1.5rem"
					},
					"name": "Extra Large",
					"size": "1.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "1.875rem"
					},
					"name": "2X Large",
					"size": "1.875rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "2.25rem"
					},
					"name": "3X Large",
					"size": "2.25rem",
					"slug": "xxx-large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "3rem"
